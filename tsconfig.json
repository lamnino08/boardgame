{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["DOM", "dom.iterable", "esnext", "ES2020"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", ".next/types/layout.tsx", "app/(dashboard)/boardgame/contributor/[contributorId]/_component", "app/(dashboard)/boardgame/contributor/[contributorId]/_component"],
  "exclude": ["node_modules"],
  "rules": {
    "no-unused-vars": "off"
  }
}
